	Journal = {J Biol Chem},
	Journal-Full = {The Journal of biological chemistry},
	Mesh = {Animals; Cell Cycle Proteins; Cell Line, Tumor; DNA-Binding Proteins; E1A-Associated p300 Protein; E2F Transcription Factors; Gene Expression Regulation; Genes, Reporter; Humans; Ligands; Mice; Nuclear Proteins; Promoter Regions, Genetic; Receptors, Aryl Hydrocarbon; Retinoblastoma Protein; S Phase; Signal Transduction; Tetrachlorodibenzodioxin; Trans-Activators; Transcription Factors; Transcription, Genetic},
	Month = {Jul},
