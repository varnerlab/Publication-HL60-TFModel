	Mesh = {Calcitriol; Cell Adhesion; Cell Differentiation; Cell Division; Cell Survival; Drug Synergism; Esterases; Humans; Leukemia, Promyelocytic, Acute; Macrophage Colony-Stimulating Factor; Macrophages; Monocytes; Phagocytosis; Tetradecanoylphorbol Acetate; Tumor Cells, Cultured},
	Month = {Oct},
