	Doi = {10.1210/mend.8.12.7708050},
	Journal = {Mol Endocrinol},
	Journal-Full = {Molecular endocrinology (Baltimore, Md.)},
	Mesh = {Base Sequence; Binding Sites; Calcitriol; Cell Line; DNA; DNA-Binding Proteins; Drug Synergism; Genes, Reporter; HeLa Cells; Host Cell Factor C1; Humans; Molecular Sequence Data; Octamer Transcription Factor-1; Osteocalcin; Osteopontin; Promoter Regions, Genetic; Receptors, Calcitriol; Repetitive Sequences, Nucleic Acid; Sialoglycoproteins; Transcription Factors; Transcription, Genetic; Transcriptional Activation; Transfection},
