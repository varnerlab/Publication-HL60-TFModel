	Pmid = {15289351},
	Pst = {ppublish},
	Title = {Up-regulation of Egr1 by 1,25-dihydroxyvitamin D3 contributes to increased expression of p35 activator of cyclin-dependent kinase 5 and consequent onset of the terminal phase of HL60 cell differentiation},
	Volume = {64},
